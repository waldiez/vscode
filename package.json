{
    "name": "waldiez-vscode",
    "displayName": "Waldiez",
    "description": "A Waldiez vscode extension",
    "version": "0.7.0",
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "AI"
    ],
    "keywords": [
        "ag2",
        "agents",
        "waldiez",
        "vscode"
    ],
    "license": "Apache-2.0",
    "publisher": "Waldiez",
    "pricing": "Free",
    "icon": "icons/logo.png",
    "contributors": [
        {
            "name": "Panagiotis Kasnesis",
            "email": "pkasnesis@waldiez.io"
        },
        {
            "name": "Lazaros Toumanidis",
            "email": "laztoum@waldiez.io"
        },
        {
            "name": "Christos Chatzigeorgiou",
            "email": "chrihatz@waldiez.io"
        }
    ],
    "activationEvents": [
        "workspaceContains: **/*.waldiez"
    ],
    "extensionDependencies": [
        "ms-python.python"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/waldiez/vscode.git"
    },
    "bugs": {
        "url": "https://github.com/waldiez/vscode/issues",
        "email": "development@waldiez.io"
    },
    "main": "./dist/extension.js",
    "contributes": {
        "keybindings": [
            {
                "command": "waldiez.vscode.newFile",
                "key": "ctrl+alt+w",
                "mac": "cmd+alt+w"
            }
        ],
        "languages": [
            {
                "id": "waldiez",
                "aliases": [
                    "Waldiez",
                    "waldiez"
                ],
                "extensions": [
                    ".waldiez"
                ],
                "icon": {
                    "light": "./icons/icon.svg",
                    "dark": "./icons/icon.svg"
                }
            }
        ],
        "customEditors": [
            {
                "viewType": "waldiez.flow",
                "displayName": "Waldiez Flow",
                "selector": [
                    {
                        "filenamePattern": "*.waldiez"
                    }
                ],
                "priority": "default"
            }
        ],
        "commands": [
            {
                "command": "waldiez.vscode.newFile",
                "title": "Waldiez flow",
                "category": "Waldiez"
            },
            {
                "command": "waldiez.vscode.toPython",
                "title": "[Waldiez] Convert to .py",
                "category": "Waldiez"
            },
            {
                "command": "waldiez.vscode.toIpynb",
                "title": "[Waldiez] Convert to .ipynb",
                "category": "Waldiez"
            }
        ],
        "menus": {
            "file/newFile": [
                {
                    "command": "waldiez.vscode.newFile",
                    "group": "Waldiez"
                }
            ],
            "explorer/context": [
                {
                    "command": "waldiez.vscode.toPython",
                    "when": "resourceFilename =~ /.*\\.waldiez$/",
                    "group": "navigation"
                },
                {
                    "command": "waldiez.vscode.toIpynb",
                    "when": "resourceFilename =~ /.*\\.waldiez$/",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "prepare": "node -e \"try{require('./.husky/install.mjs')}catch(e){}\"",
        "python": "node --import=tsx scripts/python.ts",
        "vscode:prepublish": "yarn run package",
        "before:build": "yarn clean:all",
        "build": "yarn before:build && NODE_NO_WARNINGS=1 vsce package --no-yarn && yarn after:build",
        "after:build": "node --import=tsx scripts/afterBuild.ts",
        "deploy": "yarn before:build && NODE_NO_WARNINGS=1 vsce publish --no-yarn",
        "package": "yarn clean && yarn compile:prod",
        "watch": "yarn compile:dev --watch",
        "lint": "yarn eslint:check && yarn stylelint:check && yarn prettier:check",
        "format": "yarn prettier:fix && yarn stylelint:fix && yarn eslint:fix",
        "forlint": "yarn format && yarn lint",
        "stylelint:fix": "yarn stylelint:check --fix",
        "stylelint:check": "stylelint --cache \"src/**/*.css\"",
        "prettier:check": "prettier --check src",
        "prettier:fix": "prettier --write --list-different src",
        "eslint:check": "eslint --config ./eslint.config.mjs --report-unused-disable-directives --no-warn-ignored --max-warnings 0 --cache",
        "eslint:fix": "yarn eslint:check --fix",
        "compile:dev": "webpack --config webpack.config.js --mode development",
        "compile:prod": "webpack --config webpack.config.js --mode production",
        "compile:tests": "rimraf dist/test && yarn compile:dev && tsc -p ./",
        "clean": "rimraf out dist coverage .stylelintcache .eslintcache",
        "clean:webpack_cache": "rimraf .webpack",
        "clean:all": "yarn clean && yarn clean:webpack_cache",
        "before:test": "yarn compile:tests",
        "test": "yarn before:test && c8 --config .c8rc.json node ./dist/test/runTest.js",
        "version:get": "node --import=tsx scripts/version.ts --get",
        "version:set": "node --import=tsx scripts/version.ts --set",
        "all": "yarn clean:all && yarn forlint && yarn test && yarn build && yarn version:get"
    },
    "dependencies": {
        "@vscode/python-extension": "^1.0.6"
    },
    "devDependencies": {
        "@babel/core": "^7.28.6",
        "@babel/preset-react": "^7.28.5",
        "@babel/preset-typescript": "^7.28.5",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@stylistic/eslint-plugin": "^5.7.0",
        "@trivago/prettier-plugin-sort-imports": "^6.0.2",
        "@types/babel__core": "^7.20.5",
        "@types/fs-extra": "^11.0.4",
        "@types/mocha": "^10.0.10",
        "@types/node": "^25.0.7",
        "@types/react": "^19.2.8",
        "@types/react-dom": "^19.2.3",
        "@types/sinon": "^21.0.0",
        "@types/source-map-support": "^0.5.10",
        "@types/tar-stream": "^3.1.4",
        "@types/vscode": ">=1.95.0",
        "@types/vscode-webview": "^1.57.5",
        "@types/webpack-env": "^1.18.8",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2",
        "@vscode/vsce": "^3.7.1",
        "@waldiez/react": "git+https://github.com/waldiez/waldiez.git#dev",
        "ajv": "^8.17.1",
        "all-contributors-cli": "^6.26.1",
        "babel-loader": "^10.0.0",
        "c8": "^10.1.3",
        "css-loader": "^7.1.2",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-headers": "^1.3.3",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "fs-extra": "^11.3.3",
        "glob": "^13.0.0",
        "highlight.js": "^11.11.1",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "markdown-it": "^14.1.0",
        "mini-css-extract-plugin": "^2.9.4",
        "mocha": "^11.7.5",
        "nanoid": "^5.1.6",
        "nyc": "^17.1.0",
        "prettier": "^3.7.4",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "rimraf": "^6.1.2",
        "sinon": "^21.0.1",
        "source-map-support": "^0.5.21",
        "style-loader": "^4.0.0",
        "stylelint": "^16.26.1",
        "stylelint-config-recommended": "^17.0.0",
        "stylelint-config-standard": "^39.0.1",
        "stylelint-prettier": "^5.0.3",
        "tar-stream": "^3.1.7",
        "terser-webpack-plugin": "^5.3.16",
        "tmp": "0.2.5",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0",
        "vsce": "^2.15.0",
        "vscode-test": "^1.6.1",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1",
        "xml2js": "^0.6.2"
    },
    "overrides": {
        "ajv": "^8.17.1",
        "jws": "4.0.1",
        "markdown-it": "^14.1.0",
        "tmp": "0.2.5",
        "xml2js": "^0.6.2"
    },
    "resolutions": {
        "jws": "4.0.1",
        "tmp": "0.2.5",
        "xml2js": "^0.6.2"
    },
    "lint-staged": {
        "*.{js,json,mjs,ts,jsx,tsx,css,md,yaml,yml}": [
            "yarn run format",
            "yarn run lint"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 110,
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "singleQuote": false,
        "trailingComma": "all",
        "endOfLine": "lf",
        "plugins": [
            "@trivago/prettier-plugin-sort-imports"
        ],
        "importOrderSeparation": true,
        "importOrderSortSpecifiers": true,
        "importOrder": [
            "^@fortawesome/",
            "^@xyflow/",
            "^react",
            "^react-dom",
            "^react-select",
            "^zustand",
            "^nanoid",
            "^rc-slider",
            "^@monaco-editor/react",
            "^@waldiez/",
            "^[./]"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.yml",
                    "**/*.yaml",
                    "**/*.md",
                    "**/*.css"
                ],
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "packageManager": "yarn@4.12.0"
}
